{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/USTACLI/bin/Debug/net7.0/usta-cli.dll",
      "args": [
        "rankings",
        "subscribe",
        "--name",
        "Liam Neville",
        "--format",
        "SINGLES",
        "--gender",
        "M",
        "--level",
        "4.0",
        "--section",
        "Northern California",
        "--output",
        "html"
      ],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    },
    {
      "name": "Docker .NET Core Launch",
      "type": "docker",
      "request": "launch",
      "preLaunchTask": "docker-run: debug",
      "netCore": {
        "appProject": "${workspaceFolder}/USTACLI/usta-cli.csproj"
      }
    }
  ]
}